@model dynamic

<h2>Xero Assets</h2>

<table border="1">
  <tr>
    <th>Asset Name</th>
    <th>Asset Number</th>
  </tr>
  @foreach (var item in Model) {
    <tr>
      <td>@item.AssetName</td>
      <td>@item.AssetNumber</td>
    </tr>
  }
</table>

<br>
<p>
  <button onclick="location.href='@Url.Action("Index", "Home")'">Home</button>
</p>
